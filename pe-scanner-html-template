<script src="{0}"></script>
<script>
$(document).ready(function () {{
    $('tr.expand').click(function () {{
        $(this).find('span').text(function(_, value) {{
            return value == '[-]' ? '[+]' : '[-]'
        }});
            $(this).nextUntil('tr.expand').css('display', function(i,v) {{
                return this.style.display === 'table-row' ? 'none' : 'table-row';
            }});
    }});
}});

$(document).ready(function () {{
    $('tr.collapse').click(function () {{
        $(this).find('span').text(function(_, value) {{
            return value == '[+]' ? '[-]' : '[+]'
        }});
            $(this).nextUntil('tr.collapse').css('display', function(i,v) {{
                return this.style.display === 'none' ? 'table-row' : 'none';
            }});
    }});
}});
</script>

<title>Scanned: {1}</title>

